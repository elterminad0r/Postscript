/snowflake {

	gsave
	gd 16 -8 -8 exch 0 koch

	grestore gsave
	gd 16 -8 8 exch 270 koch

	grestore gsave
	gd 16 8 -8 exch 90 koch

	grestore gsave
	gd 16 8 8 exch 180 koch

	grestore

} def

