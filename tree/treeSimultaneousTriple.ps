28.3464567 dup scale

newpath
0.5 0.5 moveto
20.5 0.5 lineto
20.5 29.2 lineto
0.5 29.2 lineto
closepath

clip

/height 29.7039 def
/width 20.9903 def

width 0.5 mul height 0.5 mul translate

/Times-Roman findfont
0.1 scalefont
setfont

0.02  setlinewidth

/i 0 def

/translateval {
	/depth exch def
	/integerval exch def

	/i i 1 add def

	0 1 depth {
		/currexp exch neg depth add def

		/digit integerval 3 currexp exp div floor def

		0 1 translate
		0.9 dup scale

		digit 0 eq {
			20 rotate
		} if

		digit 2 eq {
			340 rotate
		} if

		/integerval integerval digit 3 currexp exp mul sub def

	} for
} def

-1 1 5 {
	/d exch def
	0 1 3 d 1 add exp {
		/v exch def

		gsave
		v d translateval
		newpath
		0 0 moveto
		0 1 lineto
		stroke 

		newpath
		0 0.5 moveto
		i 20 string cvs show
		
		grestore

	} for
} for

showpage
