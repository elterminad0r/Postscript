%!PS-3.0

%%BoundingBox: 1 1 594 841
%%DocumentMedia: A4 595 842 0 () ()
%%EndComments
%%BeginFeature: *PageSize A4
<< /PageSize [595 842] >> setpagedevice
%%EndFeature

28.3464567 dup scale

newpath
0.5 0.5 moveto
20.5 0.5 lineto
20.5 29.2 lineto
0.5 29.2 lineto
closepath

gsave
0 setgray fill
grestore

clip

/height 29.7039 def
/width 20.9903 def

width 0.5 mul height 0.5 mul translate

/Times-Roman findfont
1 scalefont
setfont

1 setgray

0.02  setlinewidth

/th 1 dict def
th /val 0 put

/totdepth 7 def
/sqs 4 totdepth exp def

/hilbert {
3 dict begin

    /depth exch def

    depth 0 eq {
        th /val
            th /val get 1 add
        put

        th /val get 360 mul sqs div
            sin 1 add 2 div
        th /val get 360 mul sqs div
            120 add sin 1 add 2 div
        th /val get 360 mul sqs div
            240 add sin 1 add 2 div
        setrgbcolor

        newpath
        1 1 moveto
        1 3 lineto
        3 3 lineto
        3 1 lineto
        stroke

    } {
        /unit 1 2 depth 1 sub exp div def
        /swidth 2 depth 1 add exp def

        gsave
        270 rotate
        -0.5 0.5 scale
        depth 1 sub hilbert
        newpath
        swidth 1 sub unit mul unit moveto
        swidth 1 add unit mul unit lineto
        stroke
        grestore

        gsave
        0 2 translate
        0.5 0.5 scale
        depth 1 sub hilbert
        newpath
        unit swidth 1 sub mul unit moveto
        unit swidth 1 add mul unit lineto
        stroke
        grestore

        gsave
        2 2 translate
        0.5 0.5 scale
        depth 1 sub hilbert
        newpath
        unit swidth 1 sub mul unit moveto
        unit swidth 1 sub mul unit neg lineto
        stroke
        grestore

        gsave
        4 2 translate
        90 rotate
        -0.5 0.5 scale
        depth 1 sub hilbert
        grestore

    } ifelse

end
} def

-8 -8 translate 4 4 scale

totdepth hilbert

showpage
