%!PS-Adobe-3.0
%%Orientation: Portrait
%%DocumentMedia: s 800 800 80 ()
%%Pages: 1
%%EndComments
%%EndProlog
%%BeginSetup
  << /PageSize [800 800] /Orientation 0 >> setpagedevice
%%EndSetup
%%Page: 1 1
%%BeginPageSetup
%%EndPageSetup

gsave

/draw_graph {
    /func exch def
    /res exch def
    /max_x exch def
    /min_x exch def

    newpath
    min_x dup func moveto

    min_x res max_x { 
        dup func lineto
    } for 

    stroke
} def

0 800 1 { 360 mul 800 div sin 2 add 200 mul } draw_graph

showpage

