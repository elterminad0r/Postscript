%!PS-Adobe-3.0
%%Orientation: Portrait
%%DocumentMedia: s {screen_width} {screen_height} 80 ()
%%Pages: 1
%%EndComments
%%EndProlog
%%BeginSetup
  << /PageSize [800 800] /Orientation 0 >> setpagedevice
%%EndSetup
%%Page: 1 1
%%BeginPageSetup
%%EndPageSetup

0 setgray


/s3 3 sqrt def
/s32 s3 2 div def
/s34 s3 4 div def

/sierp {
1 dict begin
    /depth exch def
    
    newpath
    0.5 0 moveto
    0.25 s34 lineto
    0.75 s34 lineto
    closepath fill

    depth 0 eq not {
        gsave
        0.5 0.5 scale
        depth 1 sub sierp
        grestore
        gsave
        0.5 0 translate
        0.5 0.5 scale
        depth 1 sub sierp
        grestore
        gsave
        0.25 s34 translate
        0.5 0.5 scale
        depth 1 sub sierp
        grestore
    } if
end
} def

100 100 translate
600 600 scale
5 sierp

showpage

